puts "enter a number"
a= gets.chomp.to_i
b=a.to_f
puts "my age is #{b} \n  and its var type #{b.class}"
puts "enter a float"
c=gets.chomp.to_f
d=c.to_i
puts"the given number #{a} is #{a.class} is converted to #{b} as #{b.class}"
puts"the given number #{c} is #{c.class} is converted to #{d} as #{d.class}"
